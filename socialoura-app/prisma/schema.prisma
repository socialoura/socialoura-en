generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Pack {
  id        String   @id @default(cuid())
  platform  String
  type      String
  quantity  Int
  price     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("packs")
}
